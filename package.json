{
    "name": "aym-ui",
    "version": "1.1.8",
    "title": "aym-ui",
    "description": "A high quality  UI components Library with Vue.js",
    "homepage": "http://www.aiyoumi.com",
    "keywords": [
        "aiyoumi",
        "vue",
        "vue.js",
        "component",
        "components",
        "ui"
    ],
    "main": "lib/index.js",
    "style": "lib/index.css",
    "files": [
        "lib",
        "src"
    ],
    "scripts": {
        "dev": "node build/dev-server.js",
        "build:demo": "cross-env NODE_ENV=production  webpack --config build/webpack.prod.demo.conf.js  --progress ",
        "build:mui": " cross-env NODE_ENV=production  webpack --config build/webpack.prod.mui.conf.js  --progress ",
        "build:modules": "cross-env NODE_ENV=production webpack --config build/webpack.prod.modules.conf.js  --progress",
        "build": "npm run build:mui & npm run build:modules",
        "new": "node build/bin/new.js",
        "lint": "eslint --fix --ext .js,.vue src",
        "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "unit:dev": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js ",
        "jest": "jest --config test/unit/jest.conf.js --coverage",
        "jest:dev": "jest --config test/unit/jest.conf.js --watch",
        "e2e": "node test/e2e/runner.js",
        "e2e:dev": "cross-env PORT=8081 nightwatch -c test/e2e/nightwatch.conf.js --env chrome",
        "test": "npm run lint && npm run unit",
        "deploy": "node ../aym-flow/index.js",
        "deploy:stable": "node ../aym-flow/index.js --stable=true",
        "deploy:docs": "npm run build:demo && node build/bin/deploy.docs.js"
    },
    "deploy": {
        "srcFolder": "./dist/",
        "destFolder": "libs/aym-ui/",
        "serverUrl": "http://127.0.0.1:7000/upload/upfiles"
    },
    "author": "aymFE",
    "license": "MIT",
    "dependencies": {
        "save-dev": "^2.0.0",
        "vue": "^2.5.2",
        "vue-router": "^3.0.1",
        "webpack-hot-middleware": "^2.21.0"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.22.1",
        "babel-eslint": "^8.2.2",
        "babel-jest": "^22.4.1",
        "babel-loader": "^7.1.1",
